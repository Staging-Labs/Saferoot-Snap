{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "scripts": {
    "build": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "clean": "rimraf build",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md'",
    "start": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby develop --host=0.0.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": "eslint --cache --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@ethersproject/abi": "^5.7.0",
    "@metamask/providers": "9.1.0",
    "@mui/icons-material": "5.14.6",
    "@mui/material": "5.14.6",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@mui/x-data-grid": "6.11.2",
    "@rainbow-me/rainbowkit": "0.12.18",
    "@types/node": "18.17.9",
    "crypto-es": "^2.0.4",
    "dotenv": "16.3.1",
    "ethers": "5.7.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "husky": "^8.0.3",
    "joi": "^17.9.2",
    "js-cookie": "^3.0.5",
    "lint-staged": "^14.0.1",
    "lottie-react": "^2.4.0",
    "lottie-web": "^5.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-table": "^7.8.0",
    "siwe": "1.1.6",
    "styled-components": "5.3.11",
    "wagmi": "0.12.19",
    "web3-utils": "1.10.1"
  },
  "devDependencies": {
    "@metamask/eslint-config": "10.0.0",
    "@metamask/eslint-config-jest": "10.0.0",
    "@metamask/eslint-config-nodejs": "10.0.0",
    "@metamask/eslint-config-typescript": "10.0.0",
    "@svgr/webpack": "6.5.1",
    "@testing-library/dom": "8.20.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-jsdoc": "39.9.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "gatsby": "5.12.1",
    "gatsby-plugin-manifest": "5.12.0",
    "gatsby-plugin-styled-components": "6.12.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "prettier": "2.8.8",
    "prettier-plugin-packagejson": "2.4.5",
    "rimraf": "3.0.2",
    "typescript": "5.2.2"
  }
}
