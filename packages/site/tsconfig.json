{
  "compilerOptions": {
    "module": "esnext",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "preserve",
    "lib": [ "es2015", "DOM" ],
    "moduleResolution": "node",
    "paths": {
      "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
    }
  },
  "include": ["src"]
}
